{
  "name": "openapi-fetch-codegen",
  "version": "1.0.0",
  "description": "Generate typed fetch functions from OpenAPI specs",
  "main": "./dist/main.js",
  "module": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "bin": {
    "openapi-fetch-codegen": "bin/cli.js"
  },
  "files": [
    "/bin",
    "/dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsc",
    "generate": "node ./bin/cli.js ./generated/github-api.yaml --o ./generated/github/",
    "lint": "eslint --max-warnings 0",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcomuser/openapi-fetch-codegen.git"
  },
  "keywords": [
    "fetch",
    "typescript",
    "ts",
    "dts",
    "openapi",
    "codegen",
    "generation",
    "openapi 3",
    "node"
  ],
  "author": "Marco Muser",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcomuser/openapi-fetch-codegen/issues"
  },
  "homepage": "https://github.com/marcomuser/openapi-fetch-codegen#readme",
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^10.1.0",
    "openapi-typescript": "^6.2.0",
    "yargs-parser": "^21.1.1"
  },
  "devDependencies": {
    "@types/node": "^18.15.0",
    "@types/yargs-parser": "^21.0.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "prettier": "^2.8.4",
    "rimraf": "^4.4.0",
    "typescript": "^4.9.5"
  }
}
